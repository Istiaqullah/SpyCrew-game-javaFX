<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="470.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Log.newPassController">
    <children>
        <ImageView fitHeight="500.0" fitWidth="850.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/logBackground.png" />
            </image>
        </ImageView>
        <Label layoutX="328.0" layoutY="133.0" text="SpyCrew" textFill="WHITE">
            <font>
                <Font name="Arial Rounded MT Bold" size="40.0" />
            </font></Label>
        <Label layoutX="319.0" layoutY="191.0" text="Recover your account" textFill="WHITE">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Button fx:id="btn_signUp" layoutX="340.0" layoutY="382.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="153.0" style="-fx-cursor: hand;" text="Submit">
            <font>
                <Font size="13.0" />
            </font>
        </Button>
        <TextField fx:id="mail" layoutX="340.0" layoutY="291.0" promptText="New password" />
        <Button layoutX="320.0" layoutY="415.0" mnemonicParsing="false" onAction="#logIn" prefHeight="27.0" prefWidth="153.0" style="-fx-background-color: transparent; -fx-cursor: hand;" text="login" textFill="WHITE" underline="true">
            <font>
                <Font size="13.0" />
            </font>
        </Button>
        <ImageView fitHeight="201.0" fitWidth="177.0" layoutX="326.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/logo.png" />
            </image>
        </ImageView>
        <Label layoutX="348.0" layoutY="251.0" text="Enter new password" textFill="WHITE">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
